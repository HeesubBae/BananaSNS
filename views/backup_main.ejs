function jsonToValue(session, name){
	var temp;
	var data;
	try{
		temp = session.split(name,2);
		data = temp[1].split('\"',2);
		return data[0];
	}catch(err){
		console.log("jsonToValueErr : " + err);
		console.log("jsonToValueSession : " + session);
		return false;
	}
}

