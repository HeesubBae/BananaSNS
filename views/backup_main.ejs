	var info_day=$("#birth_day>option:selected").val();
	
	var save_birth=info_year+'-'+info_month+'-'+info_day;

	var postdata = {
		'userid':	id,
		'birth' :	save_birth,
		'sex' :		$("#edit_sex>option:selected").val(),
