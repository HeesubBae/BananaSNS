		'city' :	$("#edit_city").val(),
		'language' :	$("#edit_language").val(),
		'blood_type' :  $("#edit_blood_type>option:selected").val(),
		'like' :	$("#edit_like").val(),
		'hobby' :	$("#edit_hobby").val(),
		'married' :	$("#edit_married>option:selected").val()
	};
	$.post('/mypage_cover_info_update', postdata,function(data){
		alert(data.status);
	});

}

