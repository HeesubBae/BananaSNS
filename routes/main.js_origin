}


// [1_3] 커버텍스트 수정
function mypage_cover_edit_text(){
	var postdata = {
		'userid':  $("#topset_mypage").val(),
		'cover_text': $("#mypage_cover_wrap_edit_input").val(),
		
	};
	$.post('/mypage_cover_text_update', postdata,function(data){
		alert(data.status);
	});
}


// [1_4] 커버페이지 개인정보 활성화
